{
    "manifests": {
        "ajardin/blizzard-sdk-bundle": {
            "manifest": {
                "bundles": {
                    "Ajardin\\BlizzardSdkBundle\\AjardinBlizzardSdkBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "#1": "Generate your own API credentials through the Blizzard Developer Portal.",
                    "#2": "https://develop.battle.net/access/clients",
                    "BLIZZARD_CLIENT_ID": "",
                    "BLIZZARD_CLIENT_SECRET": ""
                }
            },
            "files": {
                "config/packages/ajardin_blizzard_sdk_bundle.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('ajardin_blizzard_sdk', [",
                        "        'blizzard' => [",
                        "            'client_id' => '%env(BLIZZARD_CLIENT_ID)%',",
                        "            'client_secret' => '%env(BLIZZARD_CLIENT_SECRET)%',",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "534d8c2b907368f441ad2dabff7f9dc7a3dcf914"
        }
    }
}
