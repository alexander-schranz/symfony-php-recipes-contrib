{
    "manifests": {
        "async-aws/async-aws-bundle": {
            "manifest": {
                "bundles": {
                    "AsyncAws\\Symfony\\Bundle\\AsyncAwsBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "AWS_KEY": "your-aws-key",
                    "AWS_SECRET": "your-aws-secret"
                }
            },
            "files": {
                "config/packages/async_aws.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('async_aws', [",
                        "        'config' => [",
                        "            'region' => 'eu-central-1',",
                        "            'accessKeyId' => '%env(AWS_KEY)%',",
                        "            'accessKeySecret' => '%env(AWS_SECRET)%',",
                        "        ],",
                        "        'clients' => null,",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "b66ddeffd22ba8e3e6a0f4c9534ce27313af8a3d"
        }
    }
}
