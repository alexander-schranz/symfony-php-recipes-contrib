{
    "manifests": {
        "aziz403/ux-zoom": {
            "manifest": {
                "bundles": {
                    "Aziz403\\UX\\Zoom\\UxZoomBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "ZOOM_SDK_KEY": "!ChangeMe!",
                    "ZOOM_SDK_SECRET": "!ChangeMe!"
                }
            },
            "files": {
                "config/packages/ux_zoom.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('ux_zoom', [",
                        "        'sdk' => [",
                        "            'key' => '%env(ZOOM_SDK_KEY)%',",
                        "            'secret' => '%env(ZOOM_SDK_SECRET)%',",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "9f655234b47ec69232bad6204f4565b855c59aa9"
        }
    }
}
