{
    "manifests": {
        "coka/doctrine-encrypt-bundle": {
            "manifest": {
                "bundles": {
                    "Oka\\Doctrine\\EncryptBundle\\OkaDoctrineEncryptBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "OKA_DOCTRINE_ENCRYPT_SECRET": "%generate(secret)%"
                }
            },
            "files": {
                "config/packages/oka_doctrine_encrypt.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('oka_doctrine_encrypt', [",
                        "        'secret' => '%env(OKA_DOCTRINE_ENCRYPT_SECRET)%',",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "144b0b712594a7e9b30fadd173c02ecbe1d41463"
        }
    }
}
