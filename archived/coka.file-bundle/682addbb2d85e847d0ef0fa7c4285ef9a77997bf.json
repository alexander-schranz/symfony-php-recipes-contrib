{
    "manifests": {
        "coka/file-bundle": {
            "manifest": {
                "bundles": {
                    "Oka\\FileBundle\\OkaFileBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "OKAFILE_STORAGE_ROOT_PATH": "%kernel.project_dir%/public",
                    "OKAFILE_STORAGE_WEB_SERVER_HOST": "localhost"
                }
            },
            "files": {
                "config/packages/oka_file.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('oka_file', [",
                        "        'db_driver' => 'orm',",
                        "        'storage' => [",
                        "            'root_path' => '%env(resolve:OKAFILE_STORAGE_ROOT_PATH)%',",
                        "            'webserver' => [",
                        "                'host' => '%env(resolve:OKAFILE_STORAGE_WEB_SERVER_HOST)%',",
                        "            ],",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "682addbb2d85e847d0ef0fa7c4285ef9a77997bf"
        }
    }
}
