{
    "manifests": {
        "dama/doctrine-test-bundle": {
            "manifest": {
                "bundles": {
                    "DAMA\\DoctrineTestBundle\\DAMADoctrineTestBundle": [
                        "test"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "add-lines": [
                    {
