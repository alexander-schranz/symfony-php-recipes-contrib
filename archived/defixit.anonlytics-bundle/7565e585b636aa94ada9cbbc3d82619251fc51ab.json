{
    "manifests": {
        "defixit/anonlytics-bundle": {
            "manifest": {
                "bundles": {
                    "DeFixIT\\AnonlyticsBundle\\AnonlyticsBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "ANONLYTICS_CLIENT_TOKEN": "",
                    "ANONLYTICS_SITE_TOKEN": ""
                }
            },
            "files": {
                "config/packages/anonlytics.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('anonlytics', [",
                        "        'client_token' => '%env(resolve:ANONLYTICS_CLIENT_TOKEN)%',",
                        "        'site_token' => '%env(resolve:ANONLYTICS_SITE_TOKEN)%',",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "7565e585b636aa94ada9cbbc3d82619251fc51ab"
        }
    }
}
