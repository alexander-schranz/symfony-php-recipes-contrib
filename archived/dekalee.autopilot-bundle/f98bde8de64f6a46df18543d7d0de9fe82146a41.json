{
    "manifests": {
        "dekalee/autopilot-bundle": {
            "manifest": {
                "bundles": {
                    "Dekalee\\AutopilotBundle\\DekaleeAutopilotBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "#DEKALEE_AUTOPILOT_APIKEY": "the-autopilot-api-key"
                },
