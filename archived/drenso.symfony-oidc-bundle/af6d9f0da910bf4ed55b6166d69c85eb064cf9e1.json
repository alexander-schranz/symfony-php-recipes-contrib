{
    "manifests": {
        "drenso/symfony-oidc-bundle": {
            "manifest": {
                "bundles": {
                    "Drenso\\OidcBundle\\DrensoOidcBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "OIDC_WELL_KNOWN_URL": "Enter the .well-known url for the OIDC provider",
                    "OIDC_CLIENT_ID": "Enter your OIDC client id",
                    "OIDC_CLIENT_SECRET": "Enter your OIDC client secret"
                }
            },
            "files": {
                "config/packages/drenso_oidc.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('drenso_oidc', [",
                        "        'clients' => [",
                        "            'default' => [",
                        "                'well_known_url' => '%env(OIDC_WELL_KNOWN_URL)%',",
                        "                'client_id' => '%env(OIDC_CLIENT_ID)%',",
                        "                'client_secret' => '%env(OIDC_CLIENT_SECRET)%',",
                        "            ],",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "af6d9f0da910bf4ed55b6166d69c85eb064cf9e1"
        }
    }
}
