{
    "manifests": {
        "dunglas/digital-ocean-bundle": {
            "manifest": {
                "bundles": {
                    "Dunglas\\DigitalOceanBundle\\DunglasDigitalOceanBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "#1": "Go to https://cloud.digitalocean.com/account/api/tokens to create your token",
                    "DIGITAL_OCEAN_TOKEN": "<your-access-token>"
                }
            },
            "files": {
                "config/dunglas_digital_ocean.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('dunglas_digital_ocean', [",
                        "        'connections' => [",
                        "            'default' => '%env(DIGITAL_OCEAN_TOKEN)%',",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "307d59b1551350d1fb904ee2e297fdcf90151881"
        }
    }
}
