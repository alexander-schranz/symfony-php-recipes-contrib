{
    "manifests": {
        "liuggio/statsd-client-bundle": {
            "manifest": {
                "bundles": {
                    "Liuggio\\StatsDClientBundle\\LiuggioStatsDClientBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "STATS_D_HOST": "",
                    "STATS_D_PORT": "",
                    "STATS_D_PROTOCOL": "udp"
                }
            },
            "files": {
                "config/packages/liuggio_stats_d_client.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('liuggio_stats_d_client', [",
                        "        'connection' => [",
                        "            'host' => '%env(STATS_D_HOST)%',",
                        "            'port' => '%env(STATS_D_PORT)%',",
                        "            'protocol' => '%env(STATS_D_PROTOCOL)%',",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "a7eb8b8200b86abd15946be12498de3e7e7a30b8"
        }
    }
}
