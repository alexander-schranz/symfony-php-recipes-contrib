{
    "manifests": {
        "nelmio/solarium-bundle": {
            "manifest": {
                "bundles": {
                    "Nelmio\\SolariumBundle\\NelmioSolariumBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "SOLR_HOST": "localhost",
                    "SOLR_CORE": "example"
                }
            },
            "files": {
                "config/packages/nelmio_solarium.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('nelmio_solarium', [",
                        "        'endpoints' => [",
                        "            'default' => [",
                        "                'host' => '%env(SOLR_HOST)%',",
                        "                'core' => '%env(SOLR_CORE)%',",
                        "            ],",
                        "        ],",
                        "        'clients' => [",
                        "            'default' => [",
                        "                'endpoints' => ['default'],",
                        "            ],",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "bf5b279e19e0ef7963e0af83f5b809c5eb7ac544"
        }
    }
}
