{
    "manifests": {
        "omines/antispam-bundle": {
            "manifest": {
                "bundles": {
                    "Omines\\AntiSpamBundle\\AntiSpamBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                }
            },
            "files": {
                "config/packages/antispam.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('antispam', [",
                        "        'profiles' => [",
                        "            'default' => [",
                        "                'honeypot' => 'email_address',",
                        "                'timer' => [",
                        "                    'min' => 3,",
                        "                    'max' => 3600,",
                        "                ],",
                        "            ],",
                        "        ],",
                        "    ]);",
                        "    if ($containerConfigurator->env() === 'test') {",
                        "        $containerConfigurator->extension('antispam', [",
                        "            'enabled' => false,",
                        "        ]);",
                        "    }",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "b191f68c00d4ea4e6ad13533adf5e335afcbdab9"
        }
    }
}
