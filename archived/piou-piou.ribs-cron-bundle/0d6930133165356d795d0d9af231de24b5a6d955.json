{
    "manifests": {
        "piou-piou/ribs-cron-bundle": {
            "manifest": {
                "bundles": {
                    "PiouPiou\\RibsCronBundle\\RibsCronBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "IP_CRON_EXTERNAL": "",
                    "IP_CRON_INTERNAL": "127.0.0.1"
                }
            },
            "files": {
                "config/ribs_cron.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $parameters = $containerConfigurator->parameters();",
                        "",
                        "    $parameters->set('data_directory', '%kernel.project_dir%/data/');",
                        "",
                        "    $parameters->set('ribs_cron_ip_external', '%env(IP_CRON_EXTERNAL)%');",
                        "",
                        "    $parameters->set('ribs_cron_ip_internal', '%env(IP_CRON_INTERNAL)%');",
                        "",
                        "    $parameters->set('ribs_cron', null);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "0d6930133165356d795d0d9af231de24b5a6d955"
        }
    }
}
