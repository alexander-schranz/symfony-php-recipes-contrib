{
    "manifests": {
        "sentry/sentry-symfony": {
            "manifest": {
                "bundles": {
                    "Sentry\\SentryBundle\\SentryBundle": [
                        "prod"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "SENTRY_DSN": ""
                }
            },
            "files": {
                "config/packages/prod/sentry.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('sentry', [",
                        "        'dsn' => '%env(SENTRY_DSN)%',",
                        "        'options' => [",
                        "            'excluded_exceptions' => ['Symfony\\Component\\HttpKernel\\Exception\\NotFoundHttpException', 'Symfony\\Component\\Security\\Core\\Exception\\AccessDeniedException'],",
                        "        ],",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "96cc49198021d5c5253f51296ce08e6493b31ecc"
        }
    }
}
