{
    "manifests": {
        "solvrtech/symfony-logbook": {
            "manifest": {
                "bundles": {
                    "Solvrtech\\Symfony\\Logbook\\LogbookBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "LOGBOOK_API_URL": "Logbook API url",
                    "LOGBOOK_API_KEY": "Logbook API key",
                    "LOGBOOK_LEVEL": "ERROR"
                }
            },
            "files": {
                "config/packages/logbook.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('logbook', [",
                        "        'api' => [",
                        "            'url' => '%env(LOGBOOK_API_URL)%',",
                        "            'key' => '%env(LOGBOOK_API_KEY)%',",
                        "        ],",
                        "        'level' => '%env(LOGBOOK_LEVEL)%',",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "788460fa285957715d58aceff9d3c0e94ec4cbaf"
        }
    }
}
