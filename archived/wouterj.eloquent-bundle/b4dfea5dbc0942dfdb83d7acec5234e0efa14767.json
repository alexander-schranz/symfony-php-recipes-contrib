{
    "manifests": {
        "wouterj/eloquent-bundle": {
            "manifest": {
                "bundles": {
                    "WouterJ\\EloquentBundle\\WouterJEloquentBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/",
                    "src/": "%SRC_DIR%/"
                },
                "env": {
                    "DB_CONNECTION": "mysql",
                    "DB_HOST": "127.0.0.1",
                    "DB_PORT": "3306",
                    "DB_DATABASE": "symfony",
                    "DB_USERNAME": "root",
                    "DB_PASSWORD": ""
                }
            },
            "files": {
                "config/packages/eloquent.php": {
                    "contents": [
                        "<?php",
                        "",
                        "declare(strict_types=1);",
                        "",
                        "use Symfony\\Component\\DependencyInjection\\Loader\\Configurator\\ContainerConfigurator;",
                        "",
                        "return static function (ContainerConfigurator $containerConfigurator): void {",
                        "    $containerConfigurator->extension('wouterj_eloquent', [",
                        "        'driver' => '%env(DB_CONNECTION)%',",
                        "        'host' => '%env(DB_HOST)%',",
                        "        'port' => '%env(DB_PORT)%',",
                        "        'database' => '%env(DB_DATABASE)%',",
                        "        'username' => '%env(DB_USERNAME)%',",
                        "        'password' => '%env(DB_PASSWORD)%',",
                        "    ]);",
                        "};",
                        ""
                    ],
                    "executable": false
                },
                "src/Model/.gitignore": {
                    "contents": [
                        ""
                    ],
                    "executable": false
                }
            },
            "ref": "b4dfea5dbc0942dfdb83d7acec5234e0efa14767"
        }
    }
}
