{
    "manifests": {
        "wow-apps/symfony-slack-bot": {
            "manifest": {
                "bundles": {
                    "WowApps\\SlackBundle\\WowAppsSlackBundle": [
                        "all"
                    ]
                },
                "copy-from-recipe": {
                    "config/": "%CONFIG_DIR%/"
                },
                "env": {
                    "WOW_APPS_SLACK_BOT_API_URL": ""
                }
            },
            "files": {
